{
  "builds": [
    {
      "src": "doan2/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "100mb",
        "runtime": "python3.13"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "doan2/wsgi.py"
    }
  ]
}