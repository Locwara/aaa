{
    "builds": [
        {
            "src": "doan2/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "100mb", "runtime": "python3.9" }
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "doan2/wsgi.py"
        }
    ]
}